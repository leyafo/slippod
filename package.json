{
  "name": "electron-slippod",
  "version": "0.0.3",
  "description": "I hate nodejs",
  "main": "main/main.js",
  "scripts": {
    "dev": "vite",
    "test": "mocha",
    "build": "vite build",
    "db_init": "node main/du_init.js",
    "dev_start": "NODE_ENV=development electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --config.asar=false",
    "cross_dist": "electron-builder -mwl",
    "local_release": "vite build && electron-builder --config.asar=false",
    "update_schema": "node tasks/update_schema.js",
    "install_app_deps": "./node_modules/.bin/electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "Li Yafu",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.14",
    "electron": "^24.2.0",
    "electron-builder": "^23.6.0",
    "electron-context-menu": "^3.6.1",
    "electron-rebuild": "^3.2.9",
    "electron-squirrel-startup": "^1.0.0",
    "mocha": "^10.2.0",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "better-sqlite3": "^8.5.0",
    "codemirror": "5.65.13",
    "marked": "^5.0.2"
  },
  "build": {
    "appId": "liyafu",
    "mac": {
      "category": "public.app-category.education",
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "libsimple/",
      "main",
      "dist/assets",
      "dist/resource"
    ]
  }
}
